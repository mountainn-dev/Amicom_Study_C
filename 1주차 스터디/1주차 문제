#define _CRT_SECURE_NO_WARNINGS // scanf()를 사용하기 위한 정의
#include <stdio.h> // 표준 입출력을 위한 라이브러리

int main(void)
{
	int year=365; //1년을 날짜로 표현한 변수
	double rate=3.1415926535; // 원주율

	printf("Total days in year : %d \n", year); //1년을 날짜로 표현
	printf("Curcumference rate : %lf \n", rate); // 원주율 값을 표현

	int input_A; // 사용자가 입력할 변수1
	int input_B; // 사용자가 입력할 변수2

	scanf("%d %d", &input_A, &input_B); //변수 1,2 입력
	printf("%d %d", input_A, input_B); // 변수 1,2 출력

	return 0; //main 함수 반환
}
